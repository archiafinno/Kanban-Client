n        }\r\n    },\r\n    methods: {\r\n        isEditDone(value) {\r\n            this.editForm = value\r\n        },\r\n        formEdit() {\r\n            this.editForm = !this.editForm\r\n        },\r\n        removeTask(id) {\r\n            console.log(`masuk delete`)\r\n            console.log(id)\r\n            Swal.fire({\r\n                title: 'Are you sure?',\r\n                text: \"You won't be able to revert this!\",\r\n                icon: 'warning',\r\n                showCancelButton: true,\r\n                confirmButtonColor: '#3085d6',\r\n                cancelButtonColor: '#d33',\r\n                confirmButtonText: 'Yes, delete it!'\r\n                }).then((result) => {\r\n                    if (result.value) {\r\n                        axios({\r\n                            method: 'delete',\r\n                            url: `http://localhost:3000/tasks/${id}`,\r\n                            headers: {\r\n                                token: localStorage.getItem('token')\r\n                            }\r\n                        })\r\n                        .then(result => {\r\n                            Swal.fire(\r\n                                'Deleted!',\r\n                                'Your file has been deleted.',\r\n                                'success'\r\n                                )\r\n                            console.log(result)\r\n                            socket.emit('delete')\r\n\r\n                        })\r\n                        .catch(err => {\r\n                            console.log(err)\r\n                        })\r\n\r\n                    }\r\n                })\r\n        },\r\n        refetchTasks() {\r\n            this.$emit('refetchTasks')\r\n        }\r\n    },\r\n    created() {\r\n        socket.on('deleteMention', () => {\r\n            this.$emit('refetchTasks')\r\n        })\r\n    }\r\n}\r\n</script>\r\n\r\n"},"lineCount":null}},"error":null,"hash":"0a1915d1634f8